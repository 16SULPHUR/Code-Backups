@{
    ViewData["Title"] = "College Information";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" type="text/css" href="~/css/site.css" />
</head>
<body>
    <header>
        <h1>Welcome to [Your College Name]</h1>
    </header>
    <main>
        <h2>About Us</h2>
        <p>
            [Your College Name] is committed to providing a quality education and creating a supportive learning environment. 
            Our programs are designed to equip students with the knowledge and skills needed to excel in their careers. 
            Located in [City, State], we offer a range of undergraduate and graduate programs.
        </p>
        <h2>Contact Us</h2>
        <p>
            Address: [College Address]<br />
            Phone: [College Phone Number]<br />
            Email: [College Email Address]
        </p>
    </main>
    <footer>
        <p>&copy; [Year] [Your College Name]. All rights reserved.</p>
    </footer>
</body>
</html>
